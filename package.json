{
  "name": "labnote-diagram-viewer",
  "displayName": "Labnote Diagram Viewer",
  "description": "Visualize labnote workflows as diagrams using Mermaid.js",
  "version": "0.0.2",
  "publisher": "ParkYaeSeong",
  "repository": {
    "type": "git",
    "url": "https://github.com/sblabkribb/bf_workflow_diagram.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onCommand:bf-workflow-diagram.showDiagram"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bf-workflow-diagram.showDiagram",
        "title": "LabNote: 워크플로우 다이어그램 보기"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "copy-assets": "cpx \"src/**/*.js\" out",
    "compile": "npm run copy-assets && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.80.0",
    "cpx": "^1.5.0",
    "typescript": "~5.0.0"
  }
}
